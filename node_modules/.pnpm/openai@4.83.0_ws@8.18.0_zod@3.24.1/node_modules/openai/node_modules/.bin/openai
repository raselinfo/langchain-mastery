#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rasel/stacklearner/workshop/llm/node_modules/.pnpm/openai@4.83.0_ws@8.18.0_zod@3.24.1/node_modules/openai/bin/node_modules:/Users/rasel/stacklearner/workshop/llm/node_modules/.pnpm/openai@4.83.0_ws@8.18.0_zod@3.24.1/node_modules/openai/node_modules:/Users/rasel/stacklearner/workshop/llm/node_modules/.pnpm/openai@4.83.0_ws@8.18.0_zod@3.24.1/node_modules:/Users/rasel/stacklearner/workshop/llm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rasel/stacklearner/workshop/llm/node_modules/.pnpm/openai@4.83.0_ws@8.18.0_zod@3.24.1/node_modules/openai/bin/node_modules:/Users/rasel/stacklearner/workshop/llm/node_modules/.pnpm/openai@4.83.0_ws@8.18.0_zod@3.24.1/node_modules/openai/node_modules:/Users/rasel/stacklearner/workshop/llm/node_modules/.pnpm/openai@4.83.0_ws@8.18.0_zod@3.24.1/node_modules:/Users/rasel/stacklearner/workshop/llm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli" "$@"
else
  exec node  "$basedir/../../bin/cli" "$@"
fi
